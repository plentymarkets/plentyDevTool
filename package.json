{
  "name": "plentydevtool",
  "version": "1.4.2",
  "description": "plentymarkets plugin development tool",
  "homepage": "plentymarkets.com",
  "author": {
    "name": "plentysystems AG",
    "email": "plugin-core@plentymarkets.com"
  },
  "keywords": [
    "plentymarkets",
    "development"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "postinstall": "electron-builder install-app-deps",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && run-electron . --serve",
    "test": "npm run postinstall && ng test",
    "test-headless": "npm run postinstall && ng test --watch=false --browsers ChromeHeadlessNoSandbox",
    "reinstall": "rm -rf node_modules/ && npm i",
    "lint": "eslint src/environments/**/* src/**/*.ts",
    "//": "following commands are used only for production",
    "build:prod": "npm run build -- -c production",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder --config build/electron-builder.json build --linux",
    "electron:windows": "npm run build:prod && electron-builder --config build/electron-builder.json build --windows",
    "electron:mac": "npm run build:prod && electron-builder --config build/electron-builder.json build --mac",
    "electron:all": "rm -rf release && npm run build:prod && electron-builder --config build/electron-builder.json build -mwl",
    "electron:all:publish": "rm -rf release && npm run build:prod && electron-builder --config build/electron-builder.json build -mwl -p always",
    "release:major": "npm version major && npm run electron:all:publish",
    "release:minor": "npm version minor && npm run electron:all:publish",
    "release:patch": "npm version patch && npm run electron:all:publish",
    "preversion": "bash build/checkMasterBranch.sh && npm run test-headless"
  },
  "dependencies": {
    "@electron/remote": "^2.0.7",
    "@electron/typescript-definitions": "^8.9.6",
    "@ng-bootstrap/ng-bootstrap": "^12.0.0",
    "@popperjs/core": "^2.11.2",
    "bootstrap": "^4.6.1",
    "checksum": "^0.1.1",
    "chokidar": "^3.5.2",
    "electron-log": "^4.4.6",
    "electron-updater": "^4.6.5",
    "extract-zip": "2.0.1",
    "fs-extra": "^10.0.1",
    "jquery": "^3.6.0",
    "lowdb": "^1.0.0",
    "micromatch": "^4.0.4",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^13.1.0",
    "@angular-eslint/builder": "13.1.0",
    "@angular-eslint/eslint-plugin": "13.1.0",
    "@angular-eslint/eslint-plugin-template": "13.1.0",
    "@angular-eslint/schematics": "13.1.0",
    "@angular-eslint/template-parser": "13.1.0",
    "@angular/cli": "13.2.6",
    "@angular/common": "13.2.6",
    "@angular/compiler": "13.2.6",
    "@angular/compiler-cli": "13.2.6",
    "@angular/core": "13.2.6",
    "@angular/forms": "13.2.6",
    "@angular/language-service": "13.2.6",
    "@angular/localize": "^13.2.6",
    "@angular/platform-browser": "13.2.6",
    "@angular/platform-browser-dynamic": "13.2.6",
    "@angular/router": "13.2.6",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/fs-extra": "^9.0.13",
    "@types/jasmine": "3.10.3",
    "@types/jasminewd2": "2.0.10",
    "@types/lowdb": "^1.0.11",
    "@types/micromatch": "^4.0.2",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.21",
    "@types/request": "^2.48.8",
    "@typescript-eslint/eslint-plugin": "5.15.0",
    "@typescript-eslint/parser": "5.15.0",
    "chai": "4.3.6",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "^3.21.1",
    "cross-env": "7.0.3",
    "electron": "^17.1.2",
    "electron-builder": "22.14.13",
    "electron-reload": "2.0.0-alpha.1",
    "eslint": "7.27.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "jasmine-core": "4.0.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "mocha": "9.2.2",
    "nan": "2.14.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.4.8",
    "puppeteer": "^13.5.1",
    "run-electron": "^1.0.0",
    "rxjs": "^6.6.7",
    "spectron": "15.0.0",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.5.5",
    "wait-on": "^5.3.0",
    "webdriver-manager": "12.1.8",
    "zone.js": "~0.11.5"
  },
  "engines": {
    "node": ">=12.20.0"
  }
}
