{
  "name": "plentydevtool",
  "version": "1.4.0",
  "description": "plentymarkets plugin development tool",
  "homepage": "plentymarkets.com",
  "author": {
    "name": "plentysystems AG",
    "email": "plugin-core@plentymarkets.com"
  },
  "keywords": [
    "plentymarkets",
    "development"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "postinstall": "electron-builder install-app-deps",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && run-electron . --serve",
    "test": "npm run postinstall && ng test",
    "test-headless": "npm run postinstall && ng test --watch=false --browsers ChromeHeadlessNoSandbox",
    "reinstall": "rm -rf node_modules/ && npm i",
    "lint": "eslint src/environments/**/* src/**/*.ts",
      "build:prod": "npm run build -- -c production",
      "electron:local": "npm run build:prod && electron .",
      "electron:linux": "npm run build:prod && electron-builder build --linux",
      "electron:windows": "npm run build:prod && electron-builder build --windows",
      "electron:mac": "npm run build:prod && electron-builder build --mac",
      "electron:all": "rm -rf release && npm run build:prod && electron-builder build -mwl",
      "electron:all:publish": "rm -rf release && npm run build:prod && electron-builder build -mwl -p always",
      "e2e": "npm run build:prod && mocha --timeout 300000 --require ts-node/register e2e/**/*.spec.ts",
      "release:major": "npm version major && npm run electron:all:publish",
      "release:minor": "npm version minor && npm run electron:all:publish",
      "release:patch": "npm version patch && npm run electron:all:publish",
      "preversion": "bash checkMasterBranch.sh && npm run test-headless"
  },
  "dependencies": {
    "@electron/typescript-definitions": "^8.9.4",
    "@ng-bootstrap/ng-bootstrap": "^9.1.3",
    "bootstrap": "^4.6.0",
    "checksum": "^0.1.1",
    "chokidar": "^3.5.2",
    "electron-log": "^4.3.5",
    "electron-updater": "^4.3.9",
    "extract-zip": "github:AileenCGN/extract-zip",
    "fs-extra": "^9.0.0",
    "jquery": "^3.4.1",
    "lowdb": "^1.0.0",
    "micromatch": "^4.0.2",
    "popper.js": "^1.16.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^12.1.0",
    "@angular-eslint/builder": "12.1.0",
    "@angular-eslint/eslint-plugin": "12.1.0",
    "@angular-eslint/eslint-plugin-template": "12.1.0",
    "@angular-eslint/schematics": "12.1.0",
    "@angular-eslint/template-parser": "12.1.0",
    "@angular/cli": "12.1.3",
    "@angular/common": "12.1.3",
    "@angular/compiler": "12.1.3",
    "@angular/compiler-cli": "12.1.3",
    "@angular/core": "12.1.3",
    "@angular/forms": "12.1.3",
    "@angular/language-service": "12.1.3",
    "@angular/localize": "^12.0.5",
    "@angular/platform-browser": "12.1.3",
    "@angular/platform-browser-dynamic": "12.1.3",
    "@angular/router": "12.1.3",
    "@electron/remote": "1.0.4",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "3.7.6",
    "@types/jasminewd2": "2.0.9",
    "@types/lowdb": "^1.0.10",
    "@types/micromatch": "^4.0.1",
    "@types/mocha": "8.2.2",
    "@types/node": "15.6.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "chai": "4.3.4",
    "conventional-changelog-cli": "2.1.1",
    "core-js": "^3.15.1",
    "cross-env": "7.0.3",
    "electron": "13.0.1",
    "electron-builder": "22.10.4",
    "electron-reload": "1.5.0",
    "eslint": "7.27.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsdoc": "35.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "factory.ts": "^0.5.2",
    "faker": "^5.5.3",
    "jasmine-core": "3.7.1",
    "jasmine-spec-reporter": "7.0.0",
    "karma": "6.3.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "7.0.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.6.0",
    "mocha": "8.4.0",
    "nan": "2.14.2",
    "npm-run-all": "4.1.5",
    "postcss": "^8.3.0",
    "puppeteer": "^10.1.0",
    "run-electron": "^1.0.0",
    "rxjs": "^6.6.7",
    "spectron": "15.0.0",
    "ts-node": "9.1.1",
    "tslib": "2.1.0",
    "typescript": "4.2.4",
    "wait-on": "^5.3.0",
    "webdriver-manager": "12.1.8",
    "zone.js": "~0.11.4"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
