{
  "name": "plentydevtool",
  "version": "1.5.2",
  "description": "plentymarkets plugin development tool",
  "homepage": "plentymarkets.com",
  "author": {
    "name": "plentysystems AG",
    "email": "plugin-core@plentymarkets.com"
  },
  "keywords": [
    "plentymarkets",
    "development"
  ],
  "main": "main.js",
  "private": true,
  "scripts": {
    "ng": "ng",
    "ng:serve": "ng serve",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build",
    "build:dev": "npm run build -- -c dev",
    "postinstall": "electron-builder install-app-deps",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && run-electron . --serve",
    "test": "npm run postinstall && ng test",
    "test-headless": "npm run postinstall && ng test --watch=false --browsers ChromeHeadlessNoSandbox",
    "reinstall": "rm -rf node_modules/ && npm i",
    "lint": "eslint --no-error-on-unmatched-pattern src/*",
    "//": "following commands are used only for production",
    "build:prod": "npm run build -- -c production",
    "electron:local": "npm run build:prod && electron .",
    "electron:linux": "npm run build:prod && electron-builder --config build/electron-builder.json build --linux",
    "electron:windows": "npm run build:prod && electron-builder --config build/electron-builder.json build --windows",
    "electron:mac": "npm run build:prod && electron-builder --config build/electron-builder.json build --mac",
    "electron:all": "rm -rf release && npm run build:prod && electron-builder --config build/electron-builder.json build -mwl",
    "electron:all:publish": "rm -rf release && npm run build:prod && electron-builder --config build/electron-builder.json build -mwl -p always",
    "release:major": "npm version major && npm run electron:all:publish",
    "release:minor": "npm version minor && npm run electron:all:publish",
    "release:patch": "npm version patch && npm run electron:all:publish",
    "preversion": "bash build/checkMasterBranch.sh && npm run test-headless"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.2",
    "@electron/remote": "^2.0.8",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.11.8",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^5.3.2",
    "checksum": "^1.0.0",
    "chokidar": "^3.5.3",
    "electron-log": "^4.4.6",
    "electron-updater": "^6.3.9",
    "extract-zip": "2.0.1",
    "fs-extra": "^10.0.1",
    "lowdb": "^1.0.0",
    "micromatch": "^4.0.8",
    "postcss": "^8.4.47",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^17.0.2",
    "@angular-devkit/build-angular": "^17.0.10",
    "@angular-eslint/builder": "^17.0.1",
    "@angular-eslint/schematics": "^17.0.1",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "17.0.10",
    "@angular/common": "17.0.9",
    "@angular/compiler": "17.0.9",
    "@angular/compiler-cli": "17.0.9",
    "@angular/core": "17.0.9",
    "@angular/forms": "17.0.9",
    "@angular/language-service": "16.2.10",
    "@angular/localize": "17.0.9",
    "@angular/platform-browser": "17.0.9",
    "@angular/platform-browser-dynamic": "17.0.9",
    "@angular/router": "17.0.9",
    "@faker-js/faker": "^6.0.0",
    "@ngx-translate/core": "14.0.0",
    "@ngx-translate/http-loader": "7.0.0",
    "@types/jasmine": "4.0.0",
    "@types/node": "17.0.23",
    "@typescript-eslint/eslint-plugin": "6.9.0",
    "@typescript-eslint/parser": "6.9.0",
    "core-js": "^3.21.1",
    "electron": "^22.3.25",
    "electron-builder": "^25.1.8",
    "electron-builder-squirrel-windows": "^25.1.8",
    "electron-reload": "2.0.0-alpha.1",
    "eslint": "^8.56.0",
    "factory.ts": "^1.1.0",
    "jasmine-core": "4.0.1",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-electron": "^7.2.0",
    "karma-jasmine": "4.0.1",
    "karma-jasmine-html-reporter": "1.7.0",
    "npm-run-all": "4.1.5",
    "puppeteer": "^23.5.3",
    "run-electron": "^1.0.0",
    "rxjs": "^6.6.7",
    "ts-node": "10.7.0",
    "typescript": "5.2.2",
    "wait-on": "^7.2.0",
    "zone.js": "~0.14.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
